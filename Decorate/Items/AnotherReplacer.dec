Actor FirstGradeCase1 : CustomInventory Replaces SuperShotgun
{
  radius 24
  height 24
  +THRUACTORS +FORCEXYBILLBOARD
  Inventory.Pickupsound ""
  States
  {
  Spawn:
	TNT1 A 1 A_Jump (125,"Case1","Case2","Case3")
    loop
      Case1:
		TNT1 A 0 A_SpawnItem("BulletsCase1", 1)
		Stop
	  Case2:
        TNT1 A 0 A_SpawnItem("BulletsCase2", 1)
		Stop
	  Case3:
        TNT1 A 0 A_SpawnItem("BulletsCase3", 1)
		Stop
  }
}

