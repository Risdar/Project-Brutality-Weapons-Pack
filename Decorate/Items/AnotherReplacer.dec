Actor BulletsCase1 : CustomInventory
{
  +CountItem
  +Inventory.AlwaysPickup
  Inventory.PickupMessage "Picked up a case of Puch Bullets"
  inventory.pickupsound "DSAMMO1"
  Scale 0.9
  //$Category Ammunition
  States
  {
  Spawn:
    UNI1 A 1
    Loop
  Pickup:
	TNT1 A 0 A_GiveInventory("P.Bullets", 16)
	TNT1 A 0 A_GiveInventory("NewClip", 30)
    Stop
  }
}

