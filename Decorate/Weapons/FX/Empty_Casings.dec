// Smoke
ACTOR ShotSmoke
{
	+NOGRAVITY
	+NOBLOCKMAP
	+FLOORCLIP
	+FORCEXYBILLBOARD
	+CLIENTSIDEONLY
	+NOINTERACTION
	+DONTSPLASH
	+MISSILE
	RenderStyle Add
	Scale  0.35
	Alpha  0.4
	Radius 0
	Height 0
	Speed  1
	States
	{
    Spawn:
		NULL A 1 A_SetTranslucent(0.25)
		SMKN ABCDEFGHI 2 A_FadeOut(0.005)
		SMKN JKLMNOPQR 1 A_FadeOut(0.005)
		TNT1 A 0
		Stop
    }
}
ACTOR CasingSmokes : ShotSmoke
{
	XScale 0.035
	YScale 0.060
	RenderStyle Add
	States
	{
    Spawn:
		NULL A 1 A_SetTranslucent(0.25)
		SMKN ABCDEFGHIJKLMNOPQR 1 A_FadeOut(0.005)
		TNT1 A 0
		Stop
    }
}

