ACTOR MGLinks: EMptyBrass
{
   
   Scale 0.06
   States
   {
   Spawn:
	  TNT1 A 0
	  
      MGLK ABCDEFGHABCDEFGHABCDEFGHABCDEFGH 1
      STOP

   Death:
      CLIN A 0

      LCPJ A 0 A_Jump(256,"Rest1","Rest2","Rest3","Rest4","Rest5")
      Goto Rest1
    Rest1:
      MGLK F 200
      Loop
    Rest2:
      MGLK B 200
      Loop
    Rest3:
      MGLK F 200
      Loop
    Rest4:
      MGLK B 200
      Loop
    Rest5:
      MGLK F 200
      Loop
	 
	Splash:
	TNT1 A 0
	Stop
	
	Bullet_Janitor1:
	  MGLK F 1 A_FadeOut(0.005)
	  Loop
	
	Bullet_Janitor2:
	  MGLK B 1 A_FadeOut(0.005)
	  Loop
	
	Bullet_Janitor3:
	  MGLK F 1 A_FadeOut(0.005)
	  Loop
	Bullet_Janitor4:
	  MGLK B 1 A_FadeOut(0.005)
	  Loop
	
	Bullet_Janitor5:
	  MGLK F 1 A_FadeOut(0.005)
	  Loop
   }
}


