				A_WeaponOffset(0,32);
				A_SetRoll(0);
				PB_HandleCrosshair(39);
				A_TakeInventory("PB_LockScreenTilt",1);
			}
	TNT1 A 0 A_StopSound(6)
	TNT1 A 0 A_ZoomFactor(1)
	DTLS FEDCBA 1
	TNT1 A 0 A_Lower
	Wait
	
  Select:
    TNT1 A 0 {
				A_WeaponOffset(0,32);
				A_SetRoll(0);
				PB_HandleCrosshair(39);
				A_TakeInventory("PB_LockScreenTilt",1);
			}
