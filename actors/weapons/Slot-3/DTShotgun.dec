Actor DemonTechShotgunGiver : CustomInventory
{
  Scale 0.9
  Inventory.Amount 1
  Inventory.MaxAmount 0
  Inventory.InterHubAmount 0
  Inventory.PickupSound "DTShotgun/Pickup"
  Inventory.PickupMessage "What the Hell is that Thing?"
  Obituary "imagine playing deathmatch."
  Tag "Demon-Tech Shotgun"
  States
  {
  Spawn:
	ERAG A 0 
	TNT1 A 0 
  SpawnLoop:
	ERAG A -1
	Loop
  Pickup:
	TNT1 A 0 A_JumpIfInventory("Demon Tech Shotgun",1,"JustAmmo")
  All:
	TNT1 A 0 A_GiveInventory("Demon Tech Shotgun",1)
	TNT1 A 0 A_JumpIfInventory("DemonTechShotgunGiverCheck",1,3)
	TNT1 A 0 A_GiveInventory("DemonTechShotgunGiverCheck",1)
	TNT1 A 0 A_GiveInventory("PB_DTech",40)
	TNT1 A 0
	Stop
  JustAmmo:
    TNT1 A 0 A_JumpIfInventory("DemonTechShotgunGiverCheck",1,"Null")
	TNT1 A 0 A_GiveInventory("DemonTechShotgunGiverCheck",1)
	TNT1 A 0 A_GiveInventory("PB_DTech",40)
	Fail
  }
}

Actor DemonTechShotgunSpawnerGiver : DemonTechShotgunGiver
{
  States
  {
  JustAmmo:
    TNT1 A 0 A_JumpIfInventory("DemonTechShotgunGiverCheck",1,"Null")
	TNT1 A 0 A_GiveInventory("DemonTechShotgunGiverCheck",1)
	TNT1 A 0 A_GiveInventory("PB_DTech",40)
	Stop
  }
}

Actor DemonTechShotgunGiverCheck : Inventory { Inventory.Amount 1 Inventory.MaxAmount 1 Inventory.InterHubAmount 0 }

