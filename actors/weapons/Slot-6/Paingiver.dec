Actor SeekerRocketSmokeTrail
{
    Scale 0.50
    Speed 0
    Alpha 0.11
   -NOGRAVITY
   -NOINTERACTION
    Gravity 0.005
    States
    {
    Spawn:
		TNT1 A 0
        PUF2 EEFGHIJK 4 A_FadeOut(0.005)
        PUF2 LMNOPQRSTUVWXYZ 4 A_FadeOut(0.005)
        PUF3 ABC 2 A_FadeOut(0.005)
		Goto Death
	Death:
        TNT1 A 0
        Stop
  }
}