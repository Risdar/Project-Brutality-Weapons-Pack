		M14S D 1 FAST 
		M14S E 1 FAST 
		TNT1 A 0 A_JumpIfInventory("FMJHawk",1,"RealReady")
		TNT1 A 0 A_JumpIfInventory("GoWeaponSpecialAbility", 1, "WeaponSpecial")
