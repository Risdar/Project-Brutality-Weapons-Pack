	// BD stuff for some weapons in Fire and Ice
Actor bd_Token : Inventory
{
    Inventory.MaxAmount 1
    +inventory.Undroppable
    -inventory.Invbar
    +NONETID
    +INVISIBLE
    +NOCLIP
    +NOTARGET
    +NOBLOCKMAP
    +NOINTERACTION
    Radius 0
    Height 0
    Alpha 0.0
    Mass 0
    RenderStyle "None"
}
Actor BDWeaponACtion : bd_Token {}
Actor InAction : bd_Token {}
